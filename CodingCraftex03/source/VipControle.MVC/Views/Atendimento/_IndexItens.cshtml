    </a>

    <a href="@Url.Action("Details", new {id = Model.AtendimentoId})" class="btn btn-sm btn-primary">
        <span class="glyphicon glyphicon-list"></span>
    </a>

    <a href="@Url.Action("Delete", new {id = Model.AtendimentoId})" class="btn btn-sm btn-danger">
