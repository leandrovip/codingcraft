@model VipControle.Domain.Models.Atendimento

<td class="message-auto">
    @Html.DisplayFor(modelItem => modelItem.Cliente.Fantasia)
</td>
<td class="message-auto">
    @Html.DisplayFor(modelItem => modelItem.Atendente.Nome)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.TipoAtendimento.Descricao)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.Solicitante)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.Status)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.Prioridade)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.DataCadastro)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.DataPrevisao)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.DataConclusao)
</td>
<td>
    @Html.DisplayFor(modelItem => modelItem.DataEdicao)
</td>
